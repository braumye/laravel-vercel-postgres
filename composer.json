{
	"name": "braumye/laravel-vercel-postgres",
	"description": "A Laravel package to deploy to Vercel with Postgres",
	"keywords": [
		"laravel",
		"vercel",
		"vercel-postgres"
	],
	"homepage": "https://github.com/braumye/laravel-vercel-postgres",
	"license": "MIT",
	"authors": [
		{
			"name": "braumye",
			"email": "braum.dev@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.1",
		"spatie/laravel-package-tools": "^1.14.0",
		"illuminate/contracts": "^10.0"
	},
	"require-dev": {
		"laravel/pint": "^1.0",
		"nunomaduro/collision": "^7.8",
		"spatie/laravel-ray": "^1.26"
	},
	"autoload": {
		"psr-4": {
			"Braumye\\LaravelVercelPostgres\\": "src/"
		}
	},
	"scripts": {
		"format": "vendor/bin/pint"
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true,
			"phpstan/extension-installer": true
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Braumye\\LaravelVercelPostgres\\LaravelVercelPostgresServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
